{
  "implicitDependencies": {
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    ".eslintrc.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "nx-example",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "ZjdhYzMzNWYtZTM1MC00YThhLWI2YTItNjc3YWRlOTc2ZDNhfHJlYWQtd3JpdGU=",
        "cacheableOperations": ["build", "lint", "test"],
        "parallel": 1
      }
    }
  },
  "cli": {
    "warnings": { "versionMismatch": false },
    "packageManager": "yarn",
    "analytics": false
  },
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": { "unitTestRunner": "jest" },
    "@nrwl/angular": {
      "convert-tslint-to-eslint": { "removeTSLintIfNoMoreTSLintTargets": true }
    }
  },
  "defaultProject": "products",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": { "build": { "dependsOn": ["^build"] } }
}
